        if-feature feature-list:HAVE_CFM;
        if-feature feature-list:HAVE_ONMD;
        if-feature feature-list:HAVE_CFM_Y1731;
        description "RPC to start an on-demand Loss Measurement session for an L2 bridge
                    with start time as absolute and stop time as absolute";
        input  {
            leaf bridge-id  {
                type string {
                    pattern "([1-9]|[12][0-9]|3[0-2])";
                }
                description "The bridge group name for which the MEP was associated";
            }
            leaf md-name  {
                type string {
                    length "1..43";
                }
                mandatory true;
                description "A unique identifier of a Maintenance Domain";
            }
            leaf ma-name  {
                type string {
                    length "1..6";
                }
                mandatory true;
                description "A unique identifier of a Maintenance Association";
            }
            leaf mep-id  {
                type uint16 {
                    range "1..8191";
                }
                mandatory true;
                description "The MEP ID of the source MEP";
            }
            leaf profile-name  {
                type string {
                    length "0..64";

                    pattern "[a-zA-Z0-9_-]*";
                }
                mandatory true;
                description "This attribute contains the profile name for the SOAM LM Session";
            }
            leaf rmep-id  {
                type ipi-cfm-data-types:cfm_rmep_type_t;
                mandatory true;
                description "The remote MEP can be identified by either a MAC address or a MEP ID";
            }
            leaf start-time-type  {
                type ipi-cfm-data-types:cfm_absolute_time_t;
                mandatory true;
                description "This attribute defines the session start time";
            }
            leaf start-time  {
                type string;
                mandatory true;
                description "This attribute states that the session start time should be in
                            absolute format i.e. HH:MM:SS-DD-Mon-YYYY";
            }
            leaf stop-time-type  {
                type ipi-cfm-data-types:cfm_absolute_time_t;
                mandatory true;
                description "This attribute defines the session start time";
            }
            leaf stop-time  {
                type string;
                mandatory true;
                description "This attribute states that the session stop time should be in absolute
                            format i.e. HH:MM:SS-DD-Mon-YYYY";
            }
            leaf repetition-period  {
                type yang:timeticks {
                    range "6000..4294967295";
                }
                mandatory true;
                description "This object specifies a configurable repetition time between
                            Measurement Intervals in a Measurement session in hundredths of a
                            second";
            }
        }
    }

