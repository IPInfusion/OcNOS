    grouping  profiles-grouping {
    description
        "Top level container for platform profiles, including configuration and
                state data";
    container profiles {
        description
            "Top level container for platform profiles, including configuration and
                    state data";
        container hardware-profile {
            description
                "This container defines hardware-profile configuration and state
                        information.";
            container config {
                description
                    "This container defines hardware-profile configuration.";
                uses hardware-profile-config;
            } // END of config-container definition.
            container state {
                config false;
                description
                    "This container defines hardware-profile Operational states.";
                uses hardware-profile-config;
            } // END of state-container definition.
            container filters {
                description
                    "This container defines hardware-profile filter configuration and state
                            information.";
                container config {
                    description
                        "This container defines hardware-profile filter configuration.";
                    uses hardware-profile-filters-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container defines hardware-profile filter Operational states.";
                    uses hardware-profile-filters-config;
                } // END of state-container definition.
                container tcam-utilization {
                    if-feature feature-list:HAVE_ACL;
                    config false;
                    description
                        "This container defines TCAM Utilization information.";
                    container state {
                        config false;
                        description
                            "This container defines TCAM Utilization information Operational
                                    states.";
                        uses hardware-tcam-state;
                    } // END of state-container definition.
                    container filter-groups {
                        config false;
                        description
                            "This container defines TCAM Utilization filter-groups information.";

                        list filter-group {
                            key "group-id";
                            config false;
                            description
                                "The list of named filter-group.";
                            leaf group-id  {
                                type  leafref {
                                    path  "../state/group-id";
                                }
                                description "A unique number corresponding to each TCAM group. This attribute is
                                            used solely for defining the key for the TCAM utilization filter
                                            groups object list. It's value shouldn't be interpreted.";
                            } // END of group-id definition.

                            container state {
                                config false;
                                description
                                    "This container defines filter-group information Operational states.";
                                uses hardware-tcam-filter-group-state;
                            } // END of state-container definition.
                        } // End of filter-group-list
                    } // END of filter-groups-container definition.
                } // END of tcam-utilization-container definition.
            } // END of filters-container definition.
            container statistics {
                description
                    "This container defines hardware-profile statistics configuration and
                            state information.";
                container config {
                    description
                        "This container defines hardware-profile statistics configuration.";
                    uses hardware-profiles-statistics-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container defines hardware-profile statistics Operational states.";
                    uses hardware-profiles-statistics-config;
                } // END of state-container definition.
            } // END of statistics-container definition.
        } // END of hardware-profile-container definition.
        container forwarding-profiles {
            description
                "This container defines forwarding-profiles configuration and state
                        information.";
            container config {
                if-feature feature-list:HAVE_BCM_UFT;
                description
                    "This container defines forwarding-profiles configuration.";
                uses platform-extended-forwarding-profile-config;
            } // END of config-container definition.
            container state {
                if-feature feature-list:HAVE_BCM_UFT;
                config false;
                description
                    "This container defines forwarding-profiles operational states.";
                uses platform-extended-forwarding-profile-config;
            } // END of state-container definition.
            container kaps {
                if-feature feature-list:HAVE_DUNE;
                if-feature feature-list:HAVE_BCM_UFT;
                if-feature feature-list:NOT_HAVE_DNX;
                description
                    "This container defines forwarding-profiles KAPS configuration and
                            state information.";
                container config {
                    description
                        "This container defines forwarding-profiles KAPS configuration.";
                    uses platform-kaps-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container definesforwarding-profiles KAPS Operational states.";
                    uses platform-kaps-config;
                } // END of state-container definition.
            } // END of kaps-container definition.
            container external-tcam {
                if-feature feature-list:HAVE_DUNE;
                if-feature feature-list:HAVE_BCM_UFT;
                if-feature feature-list:NOT_HAVE_DNX;
                description
                    "This container defines forwarding-profiles external-TCAM configuration
                            and state information.";
                container config {
                    description
                        "This container defines forwarding-profiles external-TCAM
                                configuration.";
                    uses platform-ext-tcam-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container definesforwarding-profiles external-TCAM Operational
                                states.";
                    uses platform-ext-tcam-config;
                } // END of state-container definition.
            } // END of external-tcam-container definition.
            container custom-tcam {
                if-feature feature-list:HAVE_DUNE;
                if-feature feature-list:HAVE_BCM_UFT;
                if-feature feature-list:NOT_HAVE_DNX;
                description
                    "This container defines forwarding-profiles custom-TCAM configuration
                            and state information.";
                container config {
                    presence "forwarding-profiles custom-tcam configuration";
                    description
                        "This container defines forwarding-profiles custom-TCAM configuration.";
                    uses platform-custom-tcam-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container defines forwarding-profiles custom-TCAM Operational
                                states.";
                    uses platform-custom-tcam-config;
                } // END of state-container definition.
            } // END of custom-tcam-container definition.
            container table-limit {
                if-feature feature-list:HAVE_BCM_UFT;
                config false;
                description
                    "This container defines profile table maximum supported values state
                            information.";

                list kaps {
                    if-feature feature-list:HAVE_DUNE;
                    key "profile";
                    config false;
                    description
                        "The list of forwarding-profiles KAPS.";
                    leaf profile  {
                        type  leafref {
                            path  "../state/profile";
                        }
                        description "Forwarding Profile KAPS Profile type.";
                    } // END of profile definition.

                    container state {
                        config false;
                        description
                            "This container defines KAPS information Operational states.";
                        uses platform-kaps-state;
                    } // END of state-container definition.
                } // End of kaps-list

                list external-tcam {
                    if-feature feature-list:HAVE_DUNE;
                    if-feature feature-list:HAVE_ELK_TCAM;
                    key "profile";
                    config false;
                    description
                        "The list of forwarding-profiles external-TCAM.";
                    leaf profile  {
                        type  leafref {
                            path  "../state/profile";
                        }
                        description "Forwarding Profile External TCAM Profile type.";
                    } // END of profile definition.

                    container state {
                        config false;
                        description
                            "This container defines external-TCAM information Operational states.";
                        uses platform-elk-tcam-state;
                    } // END of state-container definition.
                } // End of external-tcam-list
                uses state-grouping;
                uses table-sizes-grouping;
            } // END of table-limit-container definition.
            uses mdb-grouping;
            uses custom-profile-grouping;
        } // END of forwarding-profiles-container definition.
    } // END of profiles-container definition.
    } // END of profiles-grouping definition.

    grouping  hardware-grouping {
    description
        "Top level container for hardware, including configuration and state
                data";
    container hardware {
        description
            "Top level container for hardware, including configuration and state
                    data";
        container load-balance {
            if-feature feature-list:HAVE_BROADCOM;
            description
                "This container defines hardware load-balance configuration and state
                        information.";
            container config {
                presence "hardware load-balance configuration";
                description
                    "This container defines hardware load-balance configuration.";
                uses platform-extended-load-balance-config;
                uses platform-load-balance-config;
            } // END of config-container definition.
            container state {
                config false;
                description
                    "This container defines hardware load-balance Operational states.";
                uses platform-extended-load-balance-config;
                uses platform-load-balance-config;
            } // END of state-container definition.
            container l2 {
                when " /hardware/load-balance/config/enable ";
                if-feature feature-list:HAVE_L2;
                description
                    "This container defines load-balance l2 configuration and state
                            information.";
                container config {
                    description
                        "This container defines load-balance l2 configuration.";
                    uses platform-extended-load-balance-l2-config;
                    uses platform-load-balance-l2-config;
