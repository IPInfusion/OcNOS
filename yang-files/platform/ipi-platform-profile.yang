                    uses platform-load-balance-ipv4-config;
                } // END of state-container definition.
                uses inner-ipv4-grouping;
            } // END of ipv4-container definition.
            container ipv6 {
                when " /hardware/load-balance/config/enable ";
                if-feature feature-list:HAVE_L3;
                description
                    "This container defines load-balance ipv6 configuration and state
                            information.";
                container config {
                    description
                        "This container defines load-balance ipv6 configuration.";
                    uses platform-extended-load-balance-ipv6-config;
                    uses platform-load-balance-ipv6-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "This container defines load-balance ipv6 Operational states.";
                    uses platform-extended-load-balance-ipv6-config;
                    uses platform-load-balance-ipv6-config;
                } // END of state-container definition.
            } // END of ipv6-container definition.
            uses tunnel-grouping;
            uses vxlan-grouping;
            uses mpls-ler-grouping;
            uses l4-port-grouping;
            uses port-channel-grouping;
        } // END of load-balance-container definition.
        container dram-tune {
            if-feature feature-list:HAVE_BROADCOM;
            config false;
            description
                "This container defines dram-tuning-profiles information.";
            container state {
                config false;
                description
                    "This container defines operational data for dram-tune";
                leaf dram-tune-status  {
                    if-feature feature-list:HAVE_BROADCOM;
                    type  int32;
                    default "0";
                    description "Use this attribute to configure or unconfigure hardware profile
                                service-queue profile support.";
                } // END of dram-tune-status definition.

            } // END of state-container definition.
        } // END of dram-tune-container definition.
        uses platform-cpu-queue-top;
        uses mac-ageing-grouping;
        uses platform-grouping;
        uses cpu-control-grouping;
    } // END of hardware-container definition.
    } // END of hardware-grouping definition.


