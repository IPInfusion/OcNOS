                    uses platform-load-balance-l2-config;
                } // END of state-container definition.
                uses inner-l2-grouping;
            } // END of l2-container definition.
            container ipv4 {
                when " /hardware/load-balance/config/enable ";
                if-feature feature-list:HAVE_L3;
                description
                    "This container defines load-balance ipv4 configuration and state
                            information.";
                container config {
                    description
                        "This container defines load-balance ipv4 configuration.";
                    uses platform-extended-load-balance-ipv4-config;
                    uses platform-load-balance-ipv4-config;
                } // END of config-container definition.
                container state {
                    config false;
