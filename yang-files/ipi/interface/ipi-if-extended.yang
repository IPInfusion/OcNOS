    grouping subif-rewrite-config {
        description
            "Grouping for dot1q and dot1ad encapsulation data";
        leaf vlan-action  {
            // when " ../enable-pop or ../push-tpid or ../dot1q-dot1ad-tpid ";
            type  ipi-if-types:if_subif_l2_vlan_actiontype_t;
            mandatory true;
            description "This object is used to configure the rewrite action to push, pop or
                        translate the outer VLAN-id.
                        To change the rewrite config, delete and recreate the entire rewrite
                        config.";
        } // END of vlan-action definition.

        choice vlan-action-type {
            description
                "Vlan Action Types";
            case pop {

                description
                    "Pop the outer and inner vid";

                leaf enable-pop  {
                    // when " ../vlan-action='pop' ";
                    type  ipi-if-types:if_subif_rewrite_pop_type_t;
                    mandatory true;
                    description "This attribute is used to configure pop outer and inner tag.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of enable-pop definition.

            }
            case push {

                description
                    "Push the outer and inner vid for a tpid";

                leaf push-tpid  {
                    // when " ../vlan-action='push' and ../push-outer-vlan-id ";
                    type  ipi-if-types:cml_outer_tpid_type_t;
                    mandatory true;
                    description "This attribute is used to configure the tpid value for the interface
                                in the format 0xhhhh. Currently supported values are 0x8100 or 0x88a8
                                or 0x9100 or 0x9200.To change the rewrite config, delete and recreate
                                the entire rewrite config.";
                } // END of push-tpid definition.

                leaf push-outer-vlan-id  {
                    // when " ../vlan-action='push' and ../push-tpid ";
                    type  uint16 {
                         range "1..4094"  {
                            description "1..4094: Supported range 1 - 4094";
                         }
                    }
                    mandatory true;
                    description "This Object is used to configure outer VLAN ID range for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of push-outer-vlan-id definition.

                leaf push-inner-vlan-id  {
                    type  uint16 {
                         range "1..4094"  {
                            description "1..4094: Supported range 1 - 4094";
                         }
                    }
                    description "This Object is used to configure the inner VLAN ID for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of push-inner-vlan-id definition.

            }
            case translate {

                description
                    "Translate the outer vid for a tpid";

                leaf rewrite-translate-action  {
                    type  ipi-if-types:if_subif_l2_rewrite_actiontype_t;
                    mandatory true;
                    description "This object is used to configure the rewrite translate action the
                                outer VLAN-id.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of rewrite-translate-action definition.

                leaf dot1q-dot1ad-tpid  {
                    // when " ../vlan-action='translate' and ../outer-vlan-id ";
                    type  ipi-if-types:cml_outer_tpid_type_t;
                    mandatory true;
                    description "This Object is used to configure outer VLAN ID range for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of dot1q-dot1ad-tpid definition.

                leaf outer-vlan-id  {
                    // when " ../vlan-action='translate' and ../dot1q-dot1ad-tpid ";
                    type  uint16 {
                         range "1..4094"  {
                            description "1..4094: Supported range 1 - 4094";
                         }
                    }
                    mandatory true;
                    description "This Object is used to configure VLAN ID range for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of outer-vlan-id definition.

                leaf i-dot1q-dot1ad-tpid  {
                    // when " ../vlan-action='translate' and ../inner-vlan-id ";
                    type  ipi-if-types:cml_inner_tpid_type_t;
                    mandatory true;
                    description "This Object is used to configure outer VLAN ID range for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of i-dot1q-dot1ad-tpid definition.

                leaf inner-vlan-id  {
                    // when " ../vlan-action='translate' and ../i-dot1q-dot1ad-tpid ";
                    type  uint16 {
                         range "1..4094"  {
                            description "1..4094: Supported range 1 - 4094";
                         }
                    }
                    mandatory true;
                    description "This Object is used to configure VLAN ID range for rewrite.
                                To change the rewrite config, delete and recreate the entire rewrite
                                config.";
                } // END of inner-vlan-id definition.

            }
        } // END of vlan-action-type-choice definition.
    } // END of subif-rewrite-config definition.
