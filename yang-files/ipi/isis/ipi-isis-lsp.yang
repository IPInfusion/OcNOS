                        Preference";
        leaf preference  {
            type  uint32;
            description "preference of the segment-routing mapping server. It ranges
                        from 1 to 255. Default value is 128";
        } // END of preference definition.

    } // END of isis-level-lspdb-tlv-router-cap-stlv-srms-preference-state definition.
    grouping isis-level-proto-vertex-state {
        description
            "State items for ISIS Level IPv4 Protocol Vertex";
        leaf distance  {
            type  uint32;
            description "Vertex Distance";
        } // END of distance definition.

        leaf destination-system-id  {
            type  string {
                length "1..1965";
                pattern "[^?' =,>|]+";
            }
            description "Nexthop neighbor source ID";
        } // END of destination-system-id definition.

    } // END of isis-level-proto-vertex-state definition.
    grouping isis-level-proto-vertex-nexthop-state {
        description
            "State items for ISIS Level IPv4 Protocol Vertex";
        leaf nexthop-interface  {
            type  string {
                length "1..1965";
                pattern "[^?' =,>|]+";
            }
            description "Nexthop interface index";
        } // END of nexthop-interface definition.

        leaf nexthop-system-id  {
            type  string {
                length "1..1965";
                pattern "[^?' =,>|]+";
            }
            description "Nexthop neighbor source ID";
        } // END of nexthop-system-id definition.

        leaf snpa-mac-address  {
            type  cml-data-types:cml_mac_addr_t {
                length "1..1965";
            }
            description "SNPA MAC Address";
        } // END of snpa-mac-address definition.

    } // END of isis-level-proto-vertex-nexthop-state definition.

    grouping  spf-levels-grouping {
    description
        "Enclosing container for the list of spf-levels associated";
    container spf-levels {
        if-feature feature-list:HAVE_ISISD;
        description
            "Enclosing container for the list of spf-levels associated";

        list spf-level {
        // when " ipi-isis-lsp:spf-levels/spf-level/spf-delays/spf-delay/spf-min-delay ";
            key "level";
            description
                "This objectlist defines the ISIS Level SPF Parameters";
            leaf level  {
                type  leafref {
                    path  "../config/level";
                }
                description "IS-IS instance level,(1|2|1-2)";
            } // END of level definition.

            container spf-delays {
                description
                    "Enclosing container for ISIS SPF Parameters";

                list spf-delay {
                    key "spf-min-delay spf-max-delay";
                    max-elements 1;
                    description
                        "This objectlist defines the ISIS SPF Parameters";
                    leaf spf-min-delay  {
                        type  leafref {
                            path  "../config/spf-min-delay";
                        }
                        description "Minimum delay between receiving a change to SPF calculation in
                                    milliseconds. The default value is 500";
                    } // END of spf-min-delay definition.

                    leaf spf-max-delay  {
                        type  leafref {
                            path  "../config/spf-max-delay";
                        }
                        description "Maximum delay between receiving a change to SPF calculation in
                                    milliseconds. The default value is 5000";
                    } // END of spf-max-delay definition.

                    container config {
                        description
                            "This container defines the ISIS SPF Configurations";
                        uses isis-spf-config;
                    } // END of config-container definition.
                    container state {
                        config false;
                        description
                            "This container defines the ISIS SPF state information";
                        uses isis-spf-config;
                    } // END of state-container definition.
                } // End of spf-delay-list
            } // END of spf-delays-container definition.
            container config {
                description
                    "This container defines the ISIS SPF config parameters";
                uses isis-spf-level-config;
            } // END of config-container definition.
            container state {
                config false;
                description
                    "This container defines the ISIS SPF state parameters";
                uses isis-spf-level-config;
            } // END of state-container definition.
        } // End of spf-level-list
    } // END of spf-levels-container definition.
    } // END of spf-levels-grouping definition.

    grouping  levels-grouping {
    description
        "Enclosing container for the list of levels associated";
    container levels {
        if-feature feature-list:HAVE_ISISD;
        description
            "Enclosing container for the list of levels associated";

        list level {
            key "type";
            description
                "This objectlist defines the ISIS Level Parameters";
            leaf type  {
                type  leafref {
                    path  "../config/type";
                }
                description "IS-IS instance level,(1|2|3)";
            } // END of type definition.

            container config {
                description
                    "This container defines the ISIS Level Configurations";
                uses isis-level-config;
            } // END of config-container definition.
            container state {
                config false;
                description
                    "This container defines the ISIS Level State";
                uses isis-level-config;
            } // END of state-container definition.
            container authentication {
                description
                    "Configutational and state parameters for ISIS instance level
                            authentication";
                container config {
                    presence "authentication mode is needed for keychain";
                    description
                        "Configutational parameters for ISIS authentication";
                    uses isis-authentication-config;
                } // END of config-container definition.
                container state {
                    config false;
                    description
                        "State parameters for ISIS interface level authentication";
                    uses isis-authentication-config;
                } // END of state-container definition.
            } // END of authentication-container definition.
        } // End of level-list
    } // END of levels-container definition.
    } // END of levels-grouping definition.


}