            type  ipi-ospfv3-types:ospfv3_abr_type_t;
            default "cisco";
            description "Use this attribute to set an OSPFv3 Area Border Router (ABR) type.";
        } // END of area-border-type definition.

        leaf default-metric  {
            type  uint32 {
                 range "1..16777214"  {
                    description "1..16777214: Supported range 1 - 16777214";
                 }
            }
            default "20";
            description "Use this attribute to set a default metric for OSPFv3.";
        } // END of default-metric definition.

        leaf shutdown  {
            type  empty;
            description "Use this attribute to specify an OSPFv3 shutdown";
        } // END of shutdown definition.

        leaf log-adjacency-changes  {
            type  ipi-ospfv3-types:ospfv3_log_adj_opt_t;
            default "brief";
            description "Use this attribute for the the router to send a SYSLOG message when an
                        OSPFv3 neighbor goes up or down.";
        } // END of log-adjacency-changes definition.

        leaf reference-bandwidth  {
            type  uint32 {
                 range "1..4294967"  {
                    description "1..4294967: Supported range 1 - 4294967";
                 }
            }
            units "mbps";
            default "100";
            description "Use this attribute to control how OSPFv3 calculates the default metric
