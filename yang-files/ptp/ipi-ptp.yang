                 range "1..255"  {
                    description "1..255:Local priority range (default is 128)";
                 }
            }
            default "128";
            description "The local priority attribute of the local clock";
        } // END of local-priority definition.

        leaf max-steps-removed  {
            type  uint8 {
                 range "1..255"  {
                    description "1..255:Max steps removed range (default is 255)";
